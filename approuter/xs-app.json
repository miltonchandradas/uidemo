{
  "welcomeFile": "comsapui5demo/",
  "authenticationMethod": "none",
  "logout": {
    "logoutEndpoint": "/do/logout"
  },
  "routes": [
    {
      "source": "^/catalog/(.*)$",
      "authenticationType": "none",
      "destination": "odatav4_api",
      "csrfProtection": false
    },
    {
      "source": "^/V2/(.*)$",
      "authenticationType": "none",
      "destination": "northwind_api",
      "csrfProtection": false
    }
  ]
}
