<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<ObjectListItem id="professional" icon="sap-icon://competitor" title="{hobby}"
		number="{ parts: [ {path: 'prizeMoney'}, {path: 'currencyCode'} ], type: 'sap.ui.model.type.Currency', formatOptions : { showMeasure : false } }"
		type="Active" numberState="Success" numberUnit="{currencyCode}" press=".onItemSelected">
		<firstStatus>
			<ObjectStatus text="{status}" state="{= ${status} ==='active' ? 'Success' : 'Warning' }"/>
		</firstStatus>
		<ObjectAttribute text="Ranking: {ranking}"/>
		<ObjectAttribute text="Highest Ranking: {highestRanking}"/>
	</ObjectListItem>
</core:FragmentDefinition>